{
  "name": "YouTube 2015 Theme",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Browse YouTube like it's 2015!",
  "homepage_url": "https://github.com/lassekongo83",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*.youtube.com/*",
    "*://*.ggpht.com/*",
    "*://i.ytimg.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}